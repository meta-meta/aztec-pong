{
  "name": "aztec-pong",
  "version": "1.0.0",
  "description": "An Aframe-React experiment",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "preghpages": "npm run build && rm -rf gh-pages && cp -r src gh-pages && cp dist/bundle.js gh-pages",
    "ghpages": "npm run preghpages && ghpages -p gh-pages",
    "gh-pages": "npm run ghpages",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meta-meta/aztec-pong.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "homepage": "http://meta-meta.github.io/aztec-pong",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "css-loader": "^0.23.0",
    "cross-env": "^1.0.6",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "ghpages": "0.0.3",
    "json-loader": "^0.5.4",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "aframe-core": "0.1.0",
    "aframe-react": "^1.0.0",
    "babel-polyfill": "^6.3.14",
    "cannon": "^0.6.2",
    "lodash": "^4.2.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "ws": "^0.8.0"
  }
}
